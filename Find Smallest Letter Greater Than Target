class Solution {
public:
    char nextGreatestLetter(vector<char>& letters, char target) {
        char ans=letters[0];
        for(int i=0;i<letters.size();i++)
        {
            char temp=letters[i];
            if(temp>target){
                ans=temp;
                break;
            }
        }
         return ans;
       
    }
};
